{
  "name": "GithubTrigger",
  "type": "trigger",
  "execProgram": "node",
  "main": "app.js",
  "exposeRoute": true,
  "imgUrl": "github.png",
  "version": "1.1.0",
  "description": "Handle github webhooks request",
  "methods": [{
    "name": "webhook",
    "route": "post /github/webhook",
    "viewName": "Github Webhook",
    "params": [{
        "name": "REPO_URL",
        "viewName": "Repository URL",
        "type": "string"
      },
      {
        "name": "BRANCH",
        "viewName": "Branch",
        "type": "string"
      },
      {
        "name": "SECRET",
        "viewName": "Secret",
        "type": "string"
      }
    ],
    "actionString": "REPO_URL BRANCH SECRET"
  }]
}